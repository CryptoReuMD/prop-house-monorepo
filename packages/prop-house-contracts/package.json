{
  "name": "prop-house-contracts",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "@shardlabs/starknet-hardhat-plugin": "^0.6.3",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "concurrently": "^7.3.0",
    "hardhat": "^2.10.2",
    "hardhat-preprocessor": "^0.1.4",
    "prettier": "^2.7.0",
    "prettier-plugin-solidity": "^1.0.0-beta.21",
    "starknet": "^3.18.2",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "setup": "yarn users && yarn compile",
    "users": "ts-node blacksmith.ts create",
    "lint": "prettier --write '{src,test}/{,*/**/}*.sol' '{,*/**/}*.ts'",
    "clean": "forge clean && hardhat clean",
    "chain:l1": "hardhat node",
    "chain:l2": "starknet-devnet --lite-mode",
    "chain": "concurrently 'yarn chain:l1' 'yarn chain:l2'",
    "compile:l1": "forge build",
    "compile:l2": "hardhat starknet-compile src/starknet/**",
    "compile": "concurrently 'yarn compile:l1' 'yarn compile:l2'",
    "test:l1": "forge clean && forge test --optimize --optimizer-runs 1000000 -v",
    "test:l2": "hardhat test test/starknet/**/*.ts",
    "test": "yarn test:l1 && yarn test:l2"
  }
}
